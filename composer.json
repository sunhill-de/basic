{
	"name" : "sunhill/basic",
	"description" : "The sunhill basics",
	"type" : "library",
	"license" : "MIT",
	"authors" : [{
			"name" : "Klaus Dimde",
			"email" : "klaus@dimde.info"
		}
	],
	"require":{
		"laravel/framework" : "^9.2"
	},
	"require-dev":{
		 "phpunit/phpunit" : "^9.5.10",
		"orchestra/testbench": "7.x-dev"
	},
	"minimum-stability" : "dev",
	"autoload" : {
